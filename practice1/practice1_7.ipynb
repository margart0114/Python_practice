   "source": [
    "# 建立一個計算機\n",
    "num1 = float(input(\"請輸入第一個數: \"))\n",
    "op = input(\"請輸入運算符號: \")\n",
    "num2 = float(input(\"請輸入第二個數: \"))\n",
    "\n",
    "\n",
    "if op == \"+\":\n",
    "    print(num1+num2)\n",
    "elif op == \"-\":\n",
    "    print(num1-num2)\n",
    "elif op == \"*\":\n",
    "    print(num1*num2)\n",
    "elif op == \"/\":\n",
    "    print(num1/num2)\n",
    "else:\n",
    "    print(\"不支援的運算\")\n"
   ]
  }
